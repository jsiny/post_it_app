<%= render 'shared/content_title', title: 'New Category' %>
<%= render 'shared/errors', obj: @category %>

<%= form_for @category do |f| %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name, class: 'form-control' %>
  </div>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>