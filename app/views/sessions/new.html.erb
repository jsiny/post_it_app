<%= render 'shared/content_title', title: 'Log in' %>

<%= form_tag '/login' do %>
  <div class="form-group">
    <%= label_tag :username %>
    <%= text_field_tag :username, '', class: 'form-control' %>
  </div>
    <div class="form-group">
    <%= label_tag :password %>
    <%= password_field_tag :password, '', class: 'form-control' %>
  </div>
  <%= submit_tag 'Login', class: 'btn btn-primary'%>
<% end %>